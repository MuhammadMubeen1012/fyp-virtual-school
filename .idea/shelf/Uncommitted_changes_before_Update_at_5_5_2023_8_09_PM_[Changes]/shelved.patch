                personalNIC,\r\n                fatherNIC,\r\n                classroom,\r\n                courses: c\r\n            } = res;\r\n            setBForm(bForm);\r\n            setPhoto(photo);\r\n            setDegreeProof(degreeProof);\r\n            setUser(user);\r\n            setFirstName(firstName);\r\n            setLastName(lastName);\r\n            setFatherName(fatherName);\r\n            setAge(age);\r\n            setPhoneNumber(phoneNumber);\r\n            setPersonalNIC(personalNIC);\r\n            setFatherNIC(fatherNIC);\r\n            setClassroom(classroom);\r\n            setCourses(c);\r\n            getClassroomsObjects(classroom);\r\n\r\n        })\r\n    }\r\n\r\n\r\n    const getClassroomsObjects = (classrooms) => {\r\n        getClassrooms(classrooms).then((res) => {\r\n            setClassroom(res);\r\n        });\r\n    }\r\n\r\n    useEffect(() => {\r\n        if (classroom.length > 0) {\r\n            console.log('Classroom ', classroom);\r\n            if (classroom[0].students) setNumberOfStudents(classroom.map((croom) => {\r\n                if (croom.students) {\r\n                    return croom.students.length\r\n                }\r\n            }).reduce((a, b) => a + b, 0));\r\n\r\n        }\r\n    }, [classroom]);\r\n\r\n\r\n    useEffect(() => {\r\n        getTeacherObj();\r\n        getMainCourses()\r\n    }, []);\r\n\r\n    useEffect(() => {\r\n        if (courses.length > 0) {\r\n            getMainCourses();\r\n        }\r\n    }, [courses]);\r\n    useEffect(() => {\r\n        if (mainCourses.length > 0) {\r\n            setLoading(true);\r\n        }\r\n    }, [mainCourses]);\r\n\r\n    return (<LayoutTeacher>\r\n\r\n        {/*=============== Start of main ================= */}\r\n        <main>\r\n            <h1>Overview</h1>\r\n\r\n            {/* ============ Start of insights ============== */}\r\n\r\n            <div className=\"insights\">\r\n\r\n                {/*course completed*/}\r\n                <div className=\"course-completed\">\r\n                    <span className=\"material-icons-sharp\">person_outline</span>\r\n                    <div className=\"middle\">\r\n                        <div className=\"left\">\r\n                            <h3>Total Students</h3>\r\n                            <h1>{numberOfStudents}</h1>\r\n                        </div>\r\n\r\n                        {/*<div className=\"progress\">*/}\r\n                        {/*    <svg>*/}\r\n                        {/*        <circle cx={\"38\"} cy={\"38\"} r={\"36\"}></circle>*/}\r\n                        {/*    </svg>*/}\r\n\r\n                        {/*    <div className=\"number\">*/}\r\n                        {/*        <p>81%</p>*/}\r\n                        {/*    </div>*/}\r\n                        {/*</div>*/}\r\n                    </div>\r\n\r\n                </div>\r\n\r\n                {/*course progress*/}\r\n                <div className=\"course-progress\">\r\n                    <span className=\"material-icons-sharp\">analytics</span>\r\n                    <div className=\"middle\">\r\n                        <div className=\"left\">\r\n                            <h3>Your Courses</h3>\r\n                            <h1>{mainCourses.length}</h1>\r\n                        </div>\r\n\r\n                    </div>\r\n\r\n                </div>\r\n\r\n                {/*Certification*/}\r\n                <div className=\"certification\">\r\n                    <span className=\"material-icons-sharp\">analytics</span>\r\n                    <div className=\"middle\">\r\n                        <div className=\"left\">\r\n                            <h3>Your Classes</h3>\r\n                            <h1>{classroom.length}</h1>\r\n                        </div>\r\n\r\n                    </div>\r\n\r\n                </div>\r\n\r\n            </div>\r\n\r\n            {/*============ END of insights =================== */}\r\n\r\n\r\n            {/* ============= start of Courses ================= */}\r\n            <div className=\"courses-table\">\r\n\r\n                <h2>Courses</h2>\r\n                <table>\r\n                    <thead>\r\n                    <tr>\r\n                        <th>Course Title</th>\r\n                        <th>Lessons</th>\r\n                        <th>Details</th>\r\n                    </tr>\r\n                    </thead>\r\n\r\n                    <tbody>\r\n\r\n                    {loading && mainCourses.map((course, index) => {\r\n                        console.log('000----------------', course);\r\n                        return (<tr key={index}>\r\n                            <td>{course.name}</td>\r\n                            <td>{course.lessons.length}</td>\r\n                            <td><Link href={'#'}>click here</Link></td>\r\n                        </tr>)\r\n                    })}\r\n\r\n\r\n                    </tbody>\r\n                </table>\r\n\r\n                <a href=\"#\">Show All</a>\r\n\r\n            </div>\r\n            {/* ============= End of Courses  ================== */}\r\n\r\n        </main>\r\n        {/*=============== End Of Main  ==================*/}\r\n\r\n\r\n        {/*============= start of Right side*/}\r\n        <div className=\"right\">\r\n\r\n            <div className=\"profile\">\r\n                <div className=\"info\">\r\n                    <p>Hey, <b>{firstName} {lastName}</b></p>\r\n                </div>\r\n            </div>\r\n\r\n\r\n            {/*start of recent Updates*/}\r\n            <div className=\"recent-updates\">\r\n                <h2>Upcoming Activities</h2>\r\n                <div className=\"updates\">\r\n\r\n\r\n                    <div className=\"update\">\r\n                        <div className=\"profile-photo\">\r\n                            <h3>- UX</h3>\r\n                        </div>\r\n\r\n                        <div className=\"message\">\r\n                            <p>\r\n                                Assignment due 20 March\r\n                            </p>\r\n                            <small className=\"text-muted\">2 Minutes Ago</small>\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                    <div className=\"update\">\r\n                        <div className=\"profile-photo\">\r\n                            <h3>- SEO</h3>\r\n                        </div>\r\n\r\n                        <div className=\"message\">\r\n                            <p>\r\n                                Assignment due 21 March\r\n                            </p>\r\n                            <small className=\"text-muted\">2 Minutes Ago</small>\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                    <div className=\"update\">\r\n                        <div className=\"profile-photo\">\r\n                            <h3>- SE</h3>\r\n                        </div>\r\n\r\n                        <div className=\"message\">\r\n                            <p>\r\n                                Assignment due 25 March\r\n                            </p>\r\n                            <small className=\"text-muted\">2 Minutes Ago</small>\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                </div>\r\n            </div>\r\n            {/*end of recent updates*/}\r\n\r\n\r\n            {/*============= start of Upcoming Events*/}\r\n            <div className=\"upcoming-events\">\r\n                <h2>Notice Board</h2>\r\n\r\n            </div>\r\n            {/*============= end of Upcoming Events*/}\r\n\r\n\r\n        </div>\r\n        {/*============= End of left Side*/}\r\n\r\n\r\n    </LayoutTeacher>);\r\n};\r\n\r\nexport default Index;\r\n
